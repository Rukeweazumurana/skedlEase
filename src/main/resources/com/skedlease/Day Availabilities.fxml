<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fitToWidth="true" maxHeight="1.7976931348623157E308" minHeight="900.0" minWidth="820.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.skedlease.DayAvailabilityController">
   <content>
      <VBox maxHeight="1.7976931348623157E308" minHeight="900.0" minWidth="800.0">
         <children>
            <AnchorPane prefHeight="78.0" prefWidth="600.0" style="-fx-background-color: grey;">
               <children>
                  <Button fx:id="backBtn" layoutX="28.0" layoutY="27.0" mnemonicParsing="false" onAction="#back" text="BACK" />
               </children>
            </AnchorPane>
            <Label fx:id="headerLabel" prefHeight="59.0" prefWidth="652.0" text="Availability for 2008-14-02">
               <font>
                  <Font name="Arial Black" size="40.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" left="30.0" />
               </VBox.margin>
            </Label>
            <VBox fx:id="availabilityListContainer" maxHeight="1.7976931348623157E308" minHeight="200.0" minWidth="100.0" spacing="10" style="-fx-border-width: 2px; -fx-border-color: black; -fx-border-radius: 2%;">
               <padding>
                  <Insets bottom="10" left="15.0" right="5.0" top="10" />
               </padding>
               <VBox.margin>
                  <Insets left="15.0" right="5.0" />
               </VBox.margin></VBox>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button mnemonicParsing="false" onAction="#addAvailability" text="Add Availability" />
              </buttons>
            </ButtonBar>
         </children>
      </VBox>
   </content>
</ScrollPane>
